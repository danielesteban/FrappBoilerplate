<section>
	<div class="page-header">
		<h1>{{L 'examplePanel'}}</h1>
	</div>
	<div class="well">
		<form id="someData">
			<label>{{L 'someData'}}</label>
			<div class="input-group">
				<input type="text" class="form-control" name="someData" value="{{someData}}">
				<span class="input-group-btn">
					<button class="btn btn-default">{{L 'save'}}</button>
				</span>
			</div>
		</form>
		{{#if session}}
			<br><strong>{{L 'signedInAs'}}:</strong> {{session.name}}
		{{/if}}
	</div>
	<p>
		{{a 'goBack' 'home'}}
	</p>
</section>
